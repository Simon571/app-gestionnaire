{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -H 0.0.0.0",
    "dev:tauri": "cross-env NEXT_CONFIG=next.config.tauri.ts next dev --turbopack -H 0.0.0.0",
    "build": "next build",
    "build:tauri": "cross-env NEXT_CONFIG=next.config.tauri.ts NEXT_EXPORT=true next build",
    "build:vercel": "next build",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "scrape-vcm": "ts-node --project scripts/tsconfig.json scripts/scrape-vcm.ts",
    "normalize-vcm": "ts-node --project scripts/tsconfig.json scripts/normalize-vcm.ts",
    "import-vcm": "ts-node --project scripts/tsconfig.json scripts/import-vcm.ts",
    "vcm:update:fr": "npm run scrape-vcm -- --lang=fr && npm run normalize-vcm -- --lang=fr && node scripts/verify-dates.js --lang=fr && ts-node --project scripts/tsconfig.json scripts/validate-vcm.ts --lang=fr",
    "vcm:update:en": "npm run scrape-vcm -- --lang=en && npm run normalize-vcm -- --lang=en && node scripts/verify-dates.js --lang=en && ts-node --project scripts/tsconfig.json scripts/validate-vcm.ts --lang=en",
    "vcm:update:all": "npm run vcm:update:fr && npm run vcm:update:en",
    "vcm:validate:fr": "ts-node --project scripts/tsconfig.json scripts/validate-vcm.ts --lang=fr",
    "vcm:validate:en": "ts-node --project scripts/tsconfig.json scripts/validate-vcm.ts --lang=en",
    "typecheck": "tsc --noEmit",
    "tauri:dev": "tauri dev",
    "tauri:build": "npm run build:tauri && tauri build",
    "tauri:build:release": "npm run build:tauri && tauri build --bundles msi nsis",
    "generate:icons": "node scripts/generate-house-icon.js && node scripts/generate-windows-icons.js",
    "vercel:deploy": "vercel --prod",
    "postbuild": "node scripts/copy-next-static.js",
    "postinstall": "patch-package",
    "typecheck:scripts": "tsc -p scripts/tsconfig.json --noEmit",
    "sync:keys": "ts-node --project scripts/tsconfig.json scripts/publisher-sync-keys.ts",
    "test:writer": "vitest run src/lib/__tests__/publisher-sync-flutter-writer.test.ts"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.13.0",
    "@genkit-ai/next": "^1.13.0",
    "@hookform/resolvers": "^4.1.3",
    "@noble/hashes": "^2.0.1",
    "@opentelemetry/exporter-jaeger": "^2.0.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@supabase/supabase-js": "^2.80.0",
    "@tauri-apps/api": "^2.9.0",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "framer-motion": "^11.0.0",
    "genkit": "^1.13.0",
    "lucide-react": "^0.475.0",
    "meeting-schedules-parser": "^4.2.0",
    "next": "^16.1.4",
    "next-intl": "^4.3.4",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tweetnacl": "^1.0.3",
    "wav": "^1.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.7.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.13",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/wav": "^1.0.3",
    "cross-env": "^10.1.0",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.7",
    "genkit-cli": "^1.13.0",
    "playwright": "^1.55.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^1.1.4"
  }
}
